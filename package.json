{
  "name": "bpmn-sketch-miner",
  "displayName": "BPMN Sketch Miner",
  "publisher": "s-v-o",
  "description": "Show Editor content in BPMN Sketch Miner",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Programming Languages",
    "Formatters",
    "Visualization",
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/s-v-o/bpmn-sketch-miner.git"
  },
  "activationEvents": [
    "onCommand:bpmn-sketch-miner.show"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "BPMN-Sketch-Miner",
      "properties": {
        "bpmn-sketch-miner.generatorSource": {
          "type": "string",
          "default": "local",
          "enum": [
            "local",
            "hybrid",
            "web"
          ],
          "description": "Source of the used generator."
        }
      }
    },
    "commands": [
      {
        "command": "bpmn-sketch-miner.show",
        "title": "Show BPMN"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/lz-string": "^1.3.34",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "lz-string": "^1.4.4"
  }
}
